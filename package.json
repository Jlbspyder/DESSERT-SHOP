{
  "name": "dessert-shop",
  "version": "1.0.0",
  "description": "MERN stack ecommerceapplication",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install && install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "JLB 24",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "buffer": "^6.0.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.2",
    "multer": "^1.4.5-lts.1",
    "node-stdlib-browser": "^1.2.1"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.7",
    "vite-plugin-node-stdlib-browser": "^0.2.1"
  }
}
